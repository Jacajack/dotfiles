super + w
	$BROWSER

super + m
	$FILE_BROWSER

super + shift + w
	changewal

super + control + shift + w
	vchangewal

Print
	scrot -e 'sshandler "$f"'

shift + @Print
	scrot -s -e 'sshandler "$f"'

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioLowerVolume
	pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86AudioRaiseVolume
	pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ +5%
